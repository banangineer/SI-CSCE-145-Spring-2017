  public void sortByTitle() {
    for (int i = 0; movies[i] != null; ++i) {
      if (movies[i].getName().charAt(0) 
          > movies[i+1].getName().charAt(0)) {
        
      }
      else if (movies[i].getName().charAt(0) 
          < movies[i+1].getName().charAt(0)) {
        
      }
      else if (movies[i].getName().charAt(0) 
          == movies[i+1].getName().charAt(0)) {
        
      }
    }
  }
