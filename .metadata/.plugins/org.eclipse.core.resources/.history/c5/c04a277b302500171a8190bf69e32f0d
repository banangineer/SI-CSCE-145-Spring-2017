package movie_database;

public class Movie {
  private String name;
  private int year;
  private int rating;
  private String director;
  private double boxOfficeGross;
  
  public Movie() {
    this.name = "";
    this.year = 0;
    this.rating = 0;
    this.director = "";
    this.boxOfficeGross = 0.0;
  }
  
  public Movie (String aName, int aYear, int aRating,
      String aDirector, double aBOG) {
    this.name = aName;
    this.year = aYear;
    this.rating = aRating;
    this.director = aDirector;
    this.boxOfficeGross = aBOG;
  }
}
